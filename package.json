  "bugs": {
    "url": "https://github.com/TingTung93/AI-Schedule-Manager/issues"
  },
  "homepage": "https://github.com/TingTung93/AI-Schedule-Manager#readme",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "playwright": "^1.54.2"
  }

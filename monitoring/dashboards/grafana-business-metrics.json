{
  "dashboard": {
    "id": null,
    "title": "AI Schedule Manager - Business Metrics",
    "tags": ["ai-schedule-manager", "business"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "User Engagement",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_schedule_manager_user_actions_total[5m])",
            "legendFormat": "{{action}} - {{feature}}"
          }
        ],
        "yAxes": [
          {"label": "Actions/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Revenue by Tier",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_schedule_manager_revenue_total[1h])",
            "legendFormat": "{{subscription_tier}}"
          }
        ],
        "yAxes": [
          {"label": "Revenue/hour", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Schedule Optimization Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ai_schedule_manager_schedule_operations_total{status=\"success\"}[5m]) / rate(ai_schedule_manager_schedule_operations_total[5m])",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Task Completion Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ai_schedule_manager_task_operations_total{operation=\"completed\"}[5m]) / rate(ai_schedule_manager_task_operations_total{operation=\"created\"}[5m])",
            "legendFormat": "Completion Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "User Satisfaction Scores",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.5, rate(ai_schedule_manager_user_satisfaction_score_bucket[1h]))",
            "legendFormat": "{{feature}} - Median"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ai_schedule_manager_user_satisfaction_score_bucket[1h]))",
            "legendFormat": "{{feature}} - 95th percentile"
          }
        ],
        "yAxes": [
          {"label": "Satisfaction Score", "min": 1, "max": 10}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "AI Token Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_schedule_manager_ai_tokens_used_total[5m])",
            "legendFormat": "{{model}} - {{operation}}"
          }
        ],
        "yAxes": [
          {"label": "Tokens/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Feature Usage Breakdown",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (feature) (rate(ai_schedule_manager_user_actions_total[1h]))",
            "legendFormat": "{{feature}}"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "User Tier Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (user_tier) (rate(ai_schedule_manager_user_actions_total[1h]))",
            "legendFormat": "{{user_tier}}"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16}
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "1m"
  }
}